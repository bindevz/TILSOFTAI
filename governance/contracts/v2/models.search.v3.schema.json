{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "tilsoftai://contracts/models.search.v3",
  "title": "models.search.v3 response",
  "description": "AtomicQuery-compatible models.search response (RS0 schema + RS1 summary + RS2..N tables routed to display or Atomic Data Engine datasets).",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "kind",
    "schemaVersion",
    "generatedAtUtc",
    "resource",
    "data",
    "warnings"
  ],
  "properties": {
    "kind": {
      "const": "models.search.v3"
    },
    "schemaVersion": {
      "const": 3
    },
    "generatedAtUtc": {
      "type": "string"
    },
    "resource": {
      "const": "models.search"
    },
    "filtersApplied": {
      "type": "object",
      "additionalProperties": {
        "type": [
          "string",
          "null"
        ]
      }
    },
    "rejectedFilters": {
      "type": "object",
      "additionalProperties": true
    },
    "data": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "pageNumber",
        "pageSize",
        "displayTables",
        "engineDatasets"
      ],
      "properties": {
        "pageNumber": {
          "type": "integer",
          "minimum": 1
        },
        "pageSize": {
          "type": "integer",
          "minimum": 1
        },
        "totalCount": {
          "type": [
            "integer",
            "null"
          ],
          "minimum": 0
        },
        "summary": {
          "type": [
            "object",
            "null"
          ],
          "additionalProperties": true
        },
        "displayTables": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": true,
            "required": [
              "tableName",
              "schema",
              "table"
            ],
            "properties": {
              "tableName": {
                "type": "string"
              },
              "tableKind": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "schema": {
                "type": "object",
                "additionalProperties": true
              },
              "table": {
                "$ref": "tilsoftai://contracts/tabular.data.v1"
              }
            }
          }
        },
        "engineDatasets": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": true,
            "required": [
              "datasetId",
              "tableName",
              "schema",
              "preview"
            ],
            "properties": {
              "datasetId": {
                "type": "string"
              },
              "tableName": {
                "type": "string"
              },
              "tableKind": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "schema": {
                "type": "object",
                "additionalProperties": true
              },
              "expiresAtUtc": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "preview": {
                "type": "object",
                "additionalProperties": false,
                "required": [
                  "columns",
                  "rows"
                ],
                "properties": {
                  "columns": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "rows": {
                    "type": "array",
                    "items": {
                      "type": "array"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "warnings": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}